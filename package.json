{
    "name": "bootstrap-all-version-snippet-extension",
    "displayName": "Bootstrap All Version Snippet Extension",
    "description": "Get Snippets for Bootstrap 1 to 5",
    "publisher": "DylxnTN",
    "version": "1.3.1",
    "engines": {
    "icon": "images/bootstrap-logo-black.png",    
        "vscode": "^1.48.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dylxntn/bsallvsnippets"
      },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "javascript",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "css",
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    }
}
