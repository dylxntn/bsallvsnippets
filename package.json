{
    "name": "bootstrap-all-version-snippet-extension",
    "displayName": "Bootstrap All Version Snippet Extension",
    "description": "Get Snippets for Bootstrap 1 to 4",
    "version": "0.0.1",
    "engines": {
    "icon": "icon.png",    
        "vscode": "^1.48.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "javascript",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "css",
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    }
}
